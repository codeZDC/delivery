<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div id="dialog-confirm" class="hide overflowHidden">
	<!-- PAGE CONTENT BEGINS -->
	<form class="form-horizontal" role="form">
		<div class="row">
			<div class="form-group">
				<label class="col-sm-3 control-label no-padding-right"
					for="form-field-1"> 菜单名</label>

				<div class="col-sm-9">
					<input type="text" name="menuName" placeholder="菜单名"
						>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group">
				<label class="col-sm-3 control-label no-padding-right"
					for="form-field-1-1"> 备注</label>

				<div class="col-sm-9">
					<input type="text" name="remark" placeholder="备注"
						>
				</div>
			</div>
		</div>

		<div class="row">
			<div class="form-group">
				<label class="col-sm-3 control-label no-padding-right"
					for="form-field-1"> 父级菜单 </label>

				<div class="col-sm-9">
					<select >
						<option value="">无</option>
						<option value="1">运维管理</option>
						<option value="2">网上办理</option>
					</select> 
				</div>
			</div>
		</div>

		<div class="row">
			<div class="form-group">
				<label class="col-sm-3 control-label no-padding-right"
					for="form-field-1"> 菜单图标 </label>

				<div class="col-sm-9">
					<select>
						<option value=""></option>
						<option value="1">2</option>
						<option value="2">3</option>
						<option value="3">3</option>
						<option value="4">3</option>
					</select>
				</div>
			</div>
		</div>

	</form>
</div>