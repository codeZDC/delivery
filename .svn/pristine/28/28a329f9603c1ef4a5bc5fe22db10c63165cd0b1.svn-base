/*公共Css*/
.row {
	margin-right: 0px;
	margin-left: 0px;
}

.table {
	margin-bottom: 0;
}

/* modal */
#modal{
	position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
        background: rgba(0, 0, 0, 0.25);
    opacity: 1 !important;
    filter: alpha(opacity=100) !important;
    z-index: 1039 !important;
    display: none;
}
.ui-dialog .ui-dialog-content {
	overflow: hidden ;
}