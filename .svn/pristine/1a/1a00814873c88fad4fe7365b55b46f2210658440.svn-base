<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuzhi.delivery.mapper.IUserMapper">
  <resultMap id="UserResultMap" type="com.tuzhi.delivery.domain.User">
        <id column="id" property="id" />
    <result column="userName" property="userName" />
    <result column="password" property="password" />
    <result column="unitName" property="unitName" />
    <result column="concat" property="concat" />
    <result column="telephone" property="telephone" />
    <result column="phoneNumber" property="phoneNumber" />
    <result column="zipCode" property="zipCode" />
    <result column="fax" property="fax" />
    <result column="unitProperty" property="unitProperty" />
    <result column="admZone" property="admZone" />
    <result column="deliveryZone" property="deliveryZone" />
    <result column="workAddr" property="workAddr" />
    <result column="unitAddr" property="unitAddr" />
    <result column="orgCode" property="orgCode" />
    <result column="unitdesc" property="unitdesc" />
    <result column="sphereOfBus" property="sphereOfBus" />
    <result column="qualification" property="qualification" />
    <result column="certificateCode" property="certificateCode" />
    <result column="legalPerson" property="legalPerson" />
    <result column="certificateTime" property="certificateTime" />
    <result column="termOfVali" property="termOfVali" />
    <result column="userType" property="userType" />
    <result column="roleId" property="roleId" />
    <result column="reserve1" property="reserve1" />
    <result column="reserve2" property="reserve2" />

  </resultMap>
  <sql id="column_Where">
  	<trim prefix="WHERE" prefixOverrides="AND|OR">
    	    	<if test="id != null">and id=#{id}</if>
    	<if test="userName != null and userName != '' ">and userName=#{userName}</if>
    	<if test="password != null and password != '' ">and password=#{password}</if>
    	<if test="unitName != null and unitName != '' ">and unitName=#{unitName}</if>
    	<if test="concat != null and concat != '' ">and concat=#{concat}</if>
    	<if test="telephone != null and telephone != '' ">and telephone=#{telephone}</if>
    	<if test="phoneNumber != null and phoneNumber != '' ">and phoneNumber=#{phoneNumber}</if>
    	<if test="zipCode != null and zipCode != '' ">and zipCode=#{zipCode}</if>
    	<if test="fax != null and fax != '' ">and fax=#{fax}</if>
    	<if test="unitProperty != null">and unitProperty=#{unitProperty}</if>
    	<if test="admZone != null and admZone != '' ">and admZone=#{admZone}</if>
    	<if test="deliveryZone != null and deliveryZone != '' ">and deliveryZone=#{deliveryZone}</if>
    	<if test="workAddr != null and workAddr != '' ">and workAddr=#{workAddr}</if>
    	<if test="unitAddr != null and unitAddr != '' ">and unitAddr=#{unitAddr}</if>
    	<if test="orgCode != null and orgCode != '' ">and orgCode=#{orgCode}</if>
    	<if test="unitdesc != null and unitdesc != '' ">and unitdesc=#{unitdesc}</if>
    	<if test="sphereOfBus != null and sphereOfBus != '' ">and sphereOfBus=#{sphereOfBus}</if>
    	<if test="qualification != null">and qualification=#{qualification}</if>
    	<if test="certificateCode != null and certificateCode != '' ">and certificateCode=#{certificateCode}</if>
    	<if test="legalPerson != null and legalPerson != '' ">and legalPerson=#{legalPerson}</if>
    	<if test="certificateTime != null and certificateTime != '' ">and certificateTime=#{certificateTime}</if>
    	<if test="termOfVali != null and termOfVali != '' ">and termOfVali=#{termOfVali}</if>
    	<if test="userType != null">and userType=#{userType}</if>
    	<if test="roleId != null">and roleId=#{roleId}</if>
    	<if test="reserve1 != null and reserve1 != '' ">and reserve1=#{reserve1}</if>
    	<if test="reserve2 != null and reserve2 != '' ">and reserve2=#{reserve2}</if>

    </trim>
  </sql>
  <sql id="column_List">
    id,userName,password,unitName,concat,telephone,phoneNumber,zipCode,fax,unitProperty,admZone,deliveryZone,workAddr,unitAddr,orgCode,unitdesc,sphereOfBus,qualification,certificateCode,legalPerson,certificateTime,termOfVali,userType,roleId,reserve1,reserve2
  </sql>
  
  <!--
  	功能：分页查询
  -->
  <select id="findUserList" parameterType="com.tuzhi.delivery.domain.User" resultMap="UserResultMap">
    select	<include refid="column_List" />
    from t_user
    <include refid="column_Where" />
    <if test="sortOrder !=null and sortOrder !='' and sortName != null and sortName != ''">
    	order by 
    	    	<if test="'id' == sortName">  id ${sortOrder } </if>
    	<if test="'userName' == sortName">  userName ${sortOrder } </if>
    	<if test="'password' == sortName">  password ${sortOrder } </if>
    	<if test="'unitName' == sortName">  unitName ${sortOrder } </if>
    	<if test="'concat' == sortName">  concat ${sortOrder } </if>
    	<if test="'telephone' == sortName">  telephone ${sortOrder } </if>
    	<if test="'phoneNumber' == sortName">  phoneNumber ${sortOrder } </if>
    	<if test="'zipCode' == sortName">  zipCode ${sortOrder } </if>
    	<if test="'fax' == sortName">  fax ${sortOrder } </if>
    	<if test="'unitProperty' == sortName">  unitProperty ${sortOrder } </if>
    	<if test="'admZone' == sortName">  admZone ${sortOrder } </if>
    	<if test="'deliveryZone' == sortName">  deliveryZone ${sortOrder } </if>
    	<if test="'workAddr' == sortName">  workAddr ${sortOrder } </if>
    	<if test="'unitAddr' == sortName">  unitAddr ${sortOrder } </if>
    	<if test="'orgCode' == sortName">  orgCode ${sortOrder } </if>
    	<if test="'unitdesc' == sortName">  unitdesc ${sortOrder } </if>
    	<if test="'sphereOfBus' == sortName">  sphereOfBus ${sortOrder } </if>
    	<if test="'qualification' == sortName">  qualification ${sortOrder } </if>
    	<if test="'certificateCode' == sortName">  certificateCode ${sortOrder } </if>
    	<if test="'legalPerson' == sortName">  legalPerson ${sortOrder } </if>
    	<if test="'certificateTime' == sortName">  certificateTime ${sortOrder } </if>
    	<if test="'termOfVali' == sortName">  termOfVali ${sortOrder } </if>
    	<if test="'userType' == sortName">  userType ${sortOrder } </if>
    	<if test="'roleId' == sortName">  roleId ${sortOrder } </if>
    	<if test="'reserve1' == sortName">  reserve1 ${sortOrder } </if>
    	<if test="'reserve2' == sortName">  reserve2 ${sortOrder } </if>

    </if>
    
  </select>
  
</mapper>
