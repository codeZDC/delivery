import java.util.List;

public class Tz {

	private Integer code ;
	
	private String msg;
	
	private List<?> data;
	
	private boolean success;

	public Tz(Integer code, String msg , boolean success) {
		super();
		this.code = code;
		this.msg = msg;
		this.data = data;
		this.success = success;
	}
	public Tz() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Integer getCode() {
		return code;
	}
	public void setCode(Integer code) {
		this.code = code;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public List<?> getData() {
		return data;
	}
	public void setData(List<?> data) {
		this.data = data;
	}
	public boolean isSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	
	public static Tz success(){
		return success(null);
	}
	public static Tz success(String msg){
		return success(200 , msg );
	}
	public static Tz success(Integer code,String msg){
		return new Tz(code, msg , true);
	}
	public static Tz fail(){
		return success(null);
	}
	public static Tz fail(String msg){
		return success(200 , msg );
	}
	public static Tz fail(Integer code,String msg){
		return new Tz(code, msg , true);
	}
	public Tz data(List<?> data){
		this.data = data ; 
		return this;
	}
	
}
